# vim: ft=sshconfig
ForwardAgent yes

Host iley.ru
  User iley
  UserKnownHostsFile=/dev/null
  CheckHostIP=no
  StrictHostKeyChecking=no

Host office
  User istrukov
  Hostname istrukov.muc.corp.google.com

Host dione
  HostName dione.intelib.org

Host lich.iley.ru
  User iley
  LocalForward 32400 127.0.0.1:32400
  LocalForward 8080 127.0.0.1:8080
  DynamicForward 1080

Host rpi
  Hostname 10.0.0.10
  User iley
  LocalForward 5000 127.0.0.1:5000
