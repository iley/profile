if prodcertstatus --nocheck_loas2 &> /dev/null; then
  source /google/data/ro/users/is/istrukov/profile.sh
else
  read "reply?LOAS expired. Do you want to run prodaccess? (Y/n) "
  if [[ "$reply" =~ ^[Yy]?$ ]]; then
    prodaccess && echo "Restarting shell..." && exec zsh --login
  else
    echo "Cancelled."
  fi
fi

export P4DIFF="/usr/bin/vim -X '+so /usr/share/vim/google/p4diff.vim'"
export G4MULTIDIFF=1
