export LC_ALL=en_US.UTF-8
eval $(/opt/homebrew/bin/brew shellenv)

export ZCCCFG="$HOME/Toolchains/z88dk/lib/config"
export PICO_SDK_PATH="$HOME/Toolchains/pico-sdk"

autojump_file=$(brew --prefix)/etc/profile.d/autojump.sh
if [[ -e "$autojump_file" ]]; then
    source "$autojump_file"
fi

export JAVA_HOME=/opt/homebrew/opt/openjdk

export PATH="/opt/homebrew/opt/llvm/bin:$JAVA_HOME/bin:/opt/homebrew/opt/qt/bin:/opt/homebrew/opt/coreutils/libexec/gnubin:$PATH"
