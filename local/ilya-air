export LC_ALL=en_US.UTF-8
eval $(/opt/homebrew/bin/brew shellenv)
export PATH="$HOME/Toolchains/msp430-gcc/bin:$HOME/Toolchains/z88dk/bin:/opt/homebrew/opt/coreutils/libexec/gnubin:$PATH"
export ZCCCFG="$HOME/z88dk/lib/config"

autojump_file=$(brew --prefix)/etc/profile.d/autojump.sh
if [[ -e "$autojump_file" ]]; then
    source "$autojump_file"
fi
